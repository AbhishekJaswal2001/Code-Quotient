void sort4Integers(int a, int b, int c, int d)
{
  int t;
  if(a>b)
  {
    t=a;
    a=b;
    b=t;
  }
  if(c>d)
  {
    t=c;
    c=d;
    d=t;
  }
  if(b>d)
  {
    t=b;
    b=d;
    d=t;
  }
  if(a>c)
  {
    t=a;
    a=c;
    c=t;
  }
  if(b>c)
  {
    t=b;
    b=c;
    c=t;
  }
  printf("%d\n%d\n%d\n%d",a,b,c,d);
}
